// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
    relationMode = "prisma"
}

model Song {
    id          Int         @id @default(autoincrement())
    ytId        String      @unique
    file        String
    title       String
    artist      String
    duration    Int
    timestamp   String
    image       String
    releaseYear String
    users       String
    createdAt   String
}
